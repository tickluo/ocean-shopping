{
  "name": "my-vue-starter",
  "version": "0.1.0",
  "description": "Vue.js + Webpack + Gulp + Eslint",
  "main": "src/main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "karma start karma.conf.js",
    "build": "gulp build",
    "dev": "gulp dev",
    "start": "gulp watch",
    "clean": "gulp clean",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config build/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
